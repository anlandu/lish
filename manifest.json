{
  "manifest_version": 2,

  "name": "Lish",
  "version": "1.0",
  "description": "Demo",
  "permissions": ["contextMenus", "tabs"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery-1.8.1.js", "content.js", "selection.js"]
  }],

  "browser_action": {
    "default_title": "lish-test",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "background" : {
    "scripts": ["script.js"]
  }

}
